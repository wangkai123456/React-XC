{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js",
            "config-overrides.js"
        ]
    },
    "rules": {
        "jsdoc-format": false,
        "no-console": [false, "error"],
        "no-empty": [true, "allow-empty-functions"]
    }
}
